      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "scheme",
            "stacking": { "mode": "normal" }
          }
        }
      },
      "targets": [
        {
          "expr": "sum(rate(argus_prediction_requests_total[5m])) by (village_id)",
