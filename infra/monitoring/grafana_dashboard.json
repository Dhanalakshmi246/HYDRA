      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 17 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.9 },
              { "color": "green", "value": 0.95 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "1 - (sum(rate(argus_alert_delivery_failures_total[1h])) / sum(rate(argus_alert_delivery_total[1h])))",
