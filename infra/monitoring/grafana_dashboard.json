    },
    {
      "title": "Prediction Latency Distribution",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "scheme"
          }
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(argus_prediction_latency_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "p50",
          "refId": "A"
