  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "üéØ SLA Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Prediction Latency p95",
      "description": "SLA Target: < 50ms",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "max": 200,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "orange", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(argus_prediction_latency_seconds_bucket{service=\"prediction\"}[5m])) by (le)) * 1000",
          "legendFormat": "p95 latency",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Alert Dispatch Latency p95",
      "description": "SLA Target: < 30s",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "max": 120,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 20 },
              { "color": "orange", "value": 30 },
              { "color": "red", "value": 60 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(argus_alert_dispatch_latency_seconds_bucket{service=\"alert-dispatcher\"}[5m])) by (le))",
          "legendFormat": "p95 dispatch",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Village Coverage",
      "description": "% of villages with predictions < 5min old",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.9 },
              { "color": "yellow", "value": 0.95 },
              { "color": "green", "value": 0.99 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "count(argus_village_last_prediction_timestamp > (time() - 300)) / count(argus_village_last_prediction_timestamp)",
          "legendFormat": "Coverage",
          "refId": "A"
        }
      ]
    },
    {
      "title": "CHORUS Avg Trust",
      "description": "SLA Target: > 0.3 per source",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 1,
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.2 },
              { "color": "yellow", "value": 0.3 },
              { "color": "green", "value": 0.5 }
